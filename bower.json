{
  "name": "wp-theme-makotokw",
  "homepage": "https://github.com/makotokw/wp-theme-makotokw",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "google-code-prettify": "~1.0",
    "font-awesome": "~4.5",
    "html5shiv": "~3.7",
    "normalize.css": "~3.0",
    "modernizr": "~2.8"
  },
  "overrides": {
    "google-code-prettify": {
      "main": [
        "src/prettify.*"
      ]
    },
    "font-awesome": {
      "main": [
        "scss/_*.scss",
        "fonts/*.*"
      ],
      "normalize": {
        "fonts": [
          "*.otf",
          "*-webfont.*"
        ]
      }
    },
    "normalize.css": {
      "main": []
    },
    "modernizr": {
      "main": [
        "modernizr.js"
      ]
    }
  }
}
