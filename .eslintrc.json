{
  "root": true,
  "plugins": [],
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "makotokw": true,
    "prettyPrint": true,
    "FontAwesome": true
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "func-names": ["error", "never"]
  }
}